<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="io.github.qprove_p.codesnippetstash.gui.DetailPage">

    <VBox fx:id="detailContent" alignment="TOP_CENTER" spacing="10">
        <TextField fx:id="detailName" />
        <Region prefWidth="60" HBox.hgrow="ALWAYS" />
        <TextArea fx:id="detailCode" />
        <Region prefWidth="60" HBox.hgrow="ALWAYS" />
        <ListView fx:id="detailTags"/>
    </VBox>
</AnchorPane>
